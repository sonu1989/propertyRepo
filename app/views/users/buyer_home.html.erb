<p class = "notice"></p> 
<div class="row-fluid">
  <div class="span2 color_user">
    <div class="row-fluid">
      <div class="row-fluid">
        <div class="span10"><%= link_to "Edit Profile",edit_user_registration_path%></div>
        <div class="span10"><%= link_to "Search Property",search_property_users_path%></div>
      </div>  
    </div>
  </div>
  <div class="span10 span2"> 
    <div class="row-fluid">
      <div class="row-fluid">
        <div class="span2"><strong>User:</strong></div>
        <div class="span6"><%= current_user.user_type %></div>
      </div>  
      <div class="row-fluid">
        <div class="span2"><strong>Mobile No:</strong></div>
        <div class="span6"><%= current_user.mobile %></div>
      </div>  
      <div class="row-fluid">
        <div class="span2"><strong>Gender:</strong></div>
        <div class="span6"><%= current_user.gender %></div>
      </div>  
      <div class="row-fluid">
        <div class="span2"><strong>City:</strong></div>
        <div class="span6"><%= current_user.city %></div>
      </div>
    </div>  
  </div>
</div>
