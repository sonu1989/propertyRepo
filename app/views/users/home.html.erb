<div align=right style="margin-right:20px">
<p class = "notice"></p> 
  
</div>
<div align=left style="margin-left:-20px">
  <h1>Welcome to Real Estate Explorer</h1>
</div>
<% if user_signed_in? %>
<% end %>

<div class="row-fluid">
  <div class="span2 color_user">
    <div class="row-fluid">
      <div class="row-fluid">
        <div class="span10"><%= link_to "Edit Profile",edit_users_path%></div>
      </div>  
      <div class="row-fluid">
        <div class="span10"><%= link_to "Post new Property",new_propertydetail_path%></div>
      </div>  
      <div class="row-fluid">
        <div class="span10"><%= link_to "View Property", propertydetails_path %></div>
      </div>  
    </div>
  </div>
  <div class="span10"> 
    <div class="row-fluid">
      <div class="row-fluid">
        <div class="span2"><strong>User:</strong></div>
        <div class="span6"><%= current_user.user_type %></div>
      </div>  
      <div class="row-fluid">
        <div class="span2"><strong>Mobile No:</strong></div>
        <div class="span6"><%= current_user.mobile %></div>
      </div>  
      <div class="row-fluid">
        <div class="span2"><strong>Gender:</strong></div>
        <div class="span6"><%= current_user.gender %></div>
      </div>  
      <div class="row-fluid">
        <div class="span2"><strong>City:</strong></div>
        <div class="span6"><%= current_user.city %></div>
      </div>
    </div>  
  </div>
</div>
